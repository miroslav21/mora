NotSoFar = 1
CountFirst = 0
while true:
    R = 1 @ 1-a; 0
    S_1 = R @ 0.2; 0
    S_0 = (1-R) @ a; 0
    S = S_0 + S_1
    G_11 = R*S @ 0.2; 0
    G_10 = R*(1-S) @ 0.9; 0
    G_01 = (1-R)*S @ 0.25; 0
    G_00 = (1-R)*(1-S) @ 0.01; 0
    G = G_11 + G_10 + G_01 + G_00
    Obs = (1-G)
    NotSoFar = NotSoFar*(1-Obs)
    CountFirst = CountFirst + NotSoFar
