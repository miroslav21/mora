
while true:
    B = 1 @ 0.001; 0
    EQ = 1 @ 0.002; 0
    A_11 = B*EQ @ 0.95; 0
    A_10 = B*(1-EQ) @ 0.94; 0
    A_01 = (1-B)*EQ @ 0.29; 0
    A_00 = (1-B)*(1-EQ) @ 0.001; 0
    A = A_11 + A_10 + A_01 + A_00
    J_0 = A @ 0.90; 0
    J_1 = (1-A) @ 0.05; 0
    J = J_0 + J_1
    M_0 = A @ 0.70; 0
    M_1 = (1-A) @ 0.01; 0
    M = M_0 + M_1
    denom2 = M
    num2 = denom2*EQ
